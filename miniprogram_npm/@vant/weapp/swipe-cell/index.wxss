@import '../common/index.wxss';

.van-swipe-cell {
    position: relative;
    overflow: hidden
}

.van-swipe-cell__left,
.van-swipe-cell__right {
    position: absolute;
    top: 110rpx;
    height: 25%
}

.van-swipe-cell__left {
    left: 0;
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0)
}

.van-swipe-cell__right {
    right: 0;
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0)
}
