  <van-tab title="标签 1">
    <navigator url="/pages/detail/detail"
    wx:for="{{orders1}}"
    wx:key="order_number"
    class="navigator">
        <view class="tagsStyle">
          <view class="content">
            <view>订单号：{{item.natural}}</view>
            <view>订单日期:{{item.date}}</view>
            <view>文本内容:{{item.address}}</view>  
            </view>        
        </view>
    </navigator>
  </van-tab>
  <van-tab title="标签 2">
    <navigator url="/pages/detail/detail"
    wx:for="{{orders2}}"
    wx:key="order_number"
    class="navigator">
    <view class="tagsStyle">
